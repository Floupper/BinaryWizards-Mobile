{
  "expo": {
    "scheme": "mogula",
    "name": "BinaryWizards-MobileApp",
    "slug": "BinaryWizards-MobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mathieuackermann.BinaryWizardsMobileApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mathieuackermann.BinaryWizardsMobileApp"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/mogula.otf"]
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "15079b65-0202-4000-bc2b-61833fb4f2f8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "mathieuackermann"
  }
}
